    <id>ration.theme.meaningful</id>
    <name>Meaningful Theme</name>
    <vendor>ration</vendor>

    <depends>com.intellij.modules.platform</depends>

    <resource-bundle>messages.MyBundle</resource-bundle>

    <extensions defaultExtensionNs="com.intellij">
        <!--<toolWindow factoryClass="ration.theme.meaningful.toolWindow.MyToolWindowFactory" id="MyToolWindow"/>-->
        <themeProvider id="ration.theme.meaningful" path="/theme/meaningful.theme.json" />
    </extensions>
